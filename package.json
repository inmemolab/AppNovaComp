{
  "name": "AppNovaComp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prerun": "watchman watch-del-all",
    "clean:node": "rm -rf node_modules && rm -rf yarn.lock && rm -rf package-lock.json && yarn cache clean && yarn install",
    "clean:android": "yarn clean:node && cd android && ./gradlew clean && cd ..",
    "clean:ios": "yarn clean:node &&  cd ios && rm -rf Pods  && rm -rf Podfile.lock  && rm -rf tmp.xcconfig && rm -rf ~/Library/Caches/CocoaPods && cd .. && yarn pod:reset",
    "pod:reset": "cd ios && pod deintegrate && pod setup && pod install && cd ..",
    "pod:install": "cd ios && pod install && cd ..",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "yarn prerun && react-native start",
    "start:clean": "yarn prerun && react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint .",
    "prepare": "husky install",
    "format": "npx prettier --write ."
  },
  "dependencies": {
    "@react-native-masked-view/masked-view": "^0.3.1",
    "@react-navigation/devtools": "^6.0.26",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@react-navigation/stack": "^6.3.29",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.7.2",
    "react": "18.2.0",
    "react-native": "0.74.2",
    "react-native-gesture-handler": "^2.16.2",
    "react-native-mmkv-storage": "^0.9.1",
    "react-native-reanimated": "3.12.0",
    "react-native-safe-area-context": "^4.10.4",
    "react-native-screens": "^3.31.1",
    "react-native-svg": "^15.3.0",
    "react-redux": "^8.1.2",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/plugin-transform-flow-strip-types": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@babel/runtime": "^7.24.7",
    "@jest/globals": "^29.7.0",
    "@react-native/babel-preset": "0.74.84",
    "@react-native/eslint-config": "0.74.84",
    "@react-native/js-polyfills": "0.74.84",
    "@react-native/metro-config": "0.74.84",
    "@react-native/typescript-config": "0.74.84",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.12",
    "@types/node-forge": "^1.3.11",
    "@types/react": "^18.3.3",
    "@types/react-native-crypto-js": "^1.0.3",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "@types/validator": "^13.11.10",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.2.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "prettier": "3.2.5",
    "react-native-svg-transformer": "^1.4.0",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.1.4",
    "typescript": "5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4"
}
